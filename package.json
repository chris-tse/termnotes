{
  "name": "@bunkalabs/termnotes",
  "version": "0.1.8",
  "description": "Zero-config daily notes and tasks CLI for your terminal",
  "main": "src/index.ts",
  "bin": {
    "tn": "bin/tn",
    "termnotes": "bin/tn"
  },
  "type": "module",
  "scripts": {
    "build": "rm -rf dist && rolldown src/index.ts --dir dist --minify",
    "dev": "bun src/index.ts",
    "fmt": "ultracite lint && ultracite format",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cli": "bun run src/index.ts --help"
  },
  "engines": {
    "bun": ">=1.0.0"
  },
  "files": [
    "bin/",
    "src/",
    "README.md",
    "package.json"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chris-tse/termnotes.git"
  },
  "homepage": "https://github.com/chris-tse/termnotes",
  "bugs": {
    "url": "https://github.com/chris-tse/termnotes/issues"
  },
  "keywords": [
    "cli",
    "notes",
    "tasks",
    "terminal",
    "productivity",
    "markdown"
  ],
  "author": "Chris Tse",
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "2.2.0",
    "@effect/experimental": "^0.54.6",
    "@effect/language-service": "^0.36.0",
    "@effect/vitest": "^0.25.1",
    "@types/bun": "latest",
    "rolldown": "^1.0.0-beta.34",
    "ultracite": "5.2.4",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@clack/prompts": "^0.11.0",
    "@effect/cli": "^0.69.2",
    "@effect/platform": "^0.90.6",
    "@effect/platform-bun": "^0.79.0",
    "effect": "^3.17.9"
  }
}
